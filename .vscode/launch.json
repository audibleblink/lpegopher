    "version": "0.2.0",
    "configurations": [
    

        {
            "name": "Collect dir PEs",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "collect",
                "--path",
                "C:/Program Files",
                "-v",
            ],
            "envFile": "${workspaceFolder}/.env",
        },
        {
            "name": "Collect PEs",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "collect",
                "--path",
                "c:\\",
                "-v",
            ],
            "envFile": "${workspaceFolder}/.env",
        },

        {
            "name": "Process All",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-v",
                "postprocess",
                "--drop",
            ],
            "envFile": "${workspaceFolder}/.env",
        },

        {
            "name": "Process HTTP",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-v",
                "postprocess",
                "--http",
                "192.168.1.110:8888"
            ],
            "envFile": "${workspaceFolder}/.env",
        },

    ]
}