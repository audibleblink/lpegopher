    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Process Exes",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["process", "exes", "-f", "lpedata/allexes.json"],
            "envFile": "${workspaceFolder}/.env",
        },

        {
            "name": "Process Tasks",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["process", "tasks", "-f", "lpedata/tasks.json"],
            "envFile": "${workspaceFolder}/.env",
        },

        {
            "name": "Process Services",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["process", "services", "-f", "lpedata/services.json"],
            "envFile": "${workspaceFolder}/.env",
        },

        {
            "name": "No Args",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["process"],
            "envFile": "${workspaceFolder}/.env",
        },

        {
            "name": "Collect Exes",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["collect", "exes","--path", "."],
            "envFile": "${workspaceFolder}/.env",
        },
    ]
}