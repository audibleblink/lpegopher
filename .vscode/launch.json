    "version": "0.2.0",
    "configurations": [


        {
            "name": "Collect PEs",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "collect",
                "pes",
                "--path",
                "c:\\users\\user\\appdata",
                "-o",
                "data/pes.json"
            ],
            "envFile": "${workspaceFolder}/.env",
        },

        {
            "name": "Process PEs",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "process",
                "pes",
                "-f",
                "data/pes.json"
            ],
            "envFile": "${workspaceFolder}/.env",
        },

        {
            "name": "Collect Runners",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "collect",
                "runners",
                "-o",
                "data/runners.json",
            ],
            "envFile": "${workspaceFolder}/.env",
        },

        {
            "name": "Process Runners",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "process",
                "runners",
                "-f",
                "data/runners.json"
            ],
            "envFile": "${workspaceFolder}/.env",
        },
    ]
}